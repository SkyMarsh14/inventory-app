<table>
    <caption>Device List</caption>
    <thead>
        <th>Name</th>
        <th>Type</th>
        <th>Brand</th>
    </thead>
    <tbody>
        <% for(let i=0;i<data.length;i++){ %>
            <tr>
                <th><%=data[i].name %></th>
                <td><%=data[i].categoryname %></td>
                <td><%=data[i].brandname %></td>
            </tr>
            <%} %>
    </tbody>
</table>